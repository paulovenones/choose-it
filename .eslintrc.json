{
  "extends":[
     "airbnb-typescript",
     "airbnb/hooks",
     "plugin:@typescript-eslint/recommended",
     "plugin:jest/recommended",
     "plugin:prettier/recommended"
  ],
  "plugins":[
     "react",
     "@typescript-eslint",
     "jest"
  ],
  "env":{
     "browser":true,
     "es6":true,
     "jest":true
  },
  "globals":{
     "Atomics":"readonly",
     "SharedArrayBuffer":"readonly"
  },
  "parser":"@typescript-eslint/parser",
  "parserOptions":{
     "ecmaFeatures":{
        "jsx":true
     },
     "ecmaVersion":2021,
     "sourceType":"module",
     "project":"./tsconfig.json"
  },
  "rules":{
     "linebreak-style":"off",
     "prettier/prettier":[
        "error",
        {
           "endOfLine":"auto"
        }
     ],
     "react/jsx-props-no-spreading": "off",
     "import/prefer-default-export": "off",
     "react/prop-types": "off"
  }
}